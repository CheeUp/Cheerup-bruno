post {
  url: {{server_domain}}/post/{boardId}
  body: json
  auth: none
}

body:json {
  {
    // "boardId": 1, // 게시판 ID
    "title": "게시글 제목", // 게시글 제목
    "content": "게시글 내용", // 게시글 내용
    "isAnonymous": false // 익명 여부 (true: 익명, false: 실명)
  }
}

docs {
  # URL 수정 건의
  ## /post/{boardId} -> /post
  {boardId}를 그냥 body에 넣으면 안되나요?
  
